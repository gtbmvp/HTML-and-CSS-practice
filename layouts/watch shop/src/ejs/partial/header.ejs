<header class="header">
  <div class="container">
    <div class="header__wrapper">
      <div class="header__logo">
        <img
          src="./img/svg/icons.svg#logo-ico"
          class="svg-logo-ico-dims"
          width="20"
          height="20"
          alt=""
        />

        <span class="header__logo-text">CONQUEST</span>
      </div>
      <nav class="header__nav menu">
        <ul class="menu__list">
          <li class="menu__item">
            <a href="#" class="menu__link" data-goto=".popular">Каталог</a>
          </li>
          <li class="menu__item">
            <a href="#" class="menu__link" data-goto=".promo">Акции</a>
          </li>
          <li class="menu__item">
            <a href="#" class="menu__link" data-goto=".footer">Доставка</a>
          </li>
          <li class="menu__item">
            <a href="#" class="menu__link" data-goto=".news">Контакты</a>
          </li>
        </ul>
      </nav>
      <div class="header__buttons">
        <button class="header__button-item">
          <img
            src="./img/svg/icons.svg#search-ico"
            class="svg-search-ico-dims"
            width="16"
            height="16"
            alt=""
          /></button
        ><button class="header__button-item">
          <img
            src="./img/svg/icons.svg#heart-ico"
            class="svg-heart-ico-dims"
            width="21"
            height="18"
            alt=""
          /></button
        ><button class="header__button-item">
          <img
            src="./img/svg/icons.svg#cart-ico"
            class="svg-cart-ico-dims"
            width="18"
            height="16"
            alt=""
          />
        </button>
      </div>
      <div class="header__burger burger">
        <span class="burger__line"></span>
      </div>
    </div>
  </div>
</header>
